# FizzBuzz

fizz-buzz(1, 100)

@[public, static]
fn fizz-buzz (start: int, end: int)
    i = start
    while i < end
        s = i.ToString()
        if i % 3 == 0 and i % 5 == 0
            s = "FizzBuzz"
        elif i % 3 == 0
            s = "Fizz"
        elif i % 5 == 0
            s = "Buzz"
        i++
        Console.WriteLine(s)